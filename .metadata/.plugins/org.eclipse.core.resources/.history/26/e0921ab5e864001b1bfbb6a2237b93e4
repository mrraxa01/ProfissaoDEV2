package snake;

import javax.swing.JFrame;

public class GameWindow extends JFrame {


	private static final long serialVersionUID = 1L;

	public GameWindow() {
		//titulo
		setTitle("Snake Game!");
		//tamanho da janela
		setSize(400, 200);
		// abre centralizado
		setLocationRelativeTo(null);
		//sair ao fechar
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		//visível
		setVisible(true);
	}
	
}
